{
  "name": "@aspectjs/core",
  "version": "1.0.0",
  "license": "MIT",
  "main": "bundles/aspectjs-core.umd.js",
  "unpgk": "bundles/aspectjs-core.umd.min.js",
  "module": "esm/aspectjs-core.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasThierion/aspectjs.git"
  },
  "sideEffects": false,
  "typings": "./esm/index.d.ts",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "test:angular": "cd tests/angular && npm install ../../dist && npm run test",
    "dev": "nodemon  --ignore dist --ext ts --exec 'npm run test && npm run size'",
    "build": "rollup --config rollup.config.js ",
    "size": "rollup --config rollup.config.js && size-limit",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@size-limit/preset-big-lib": "^2.2.1",
    "@types/jasmine": "^3.4.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.7.5",
    "@types/rollup": "^0.54.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^4.2.1",
    "jasmine": "^3.4.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-ts": "^0.3.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-visualizer": "^3.3.1",
    "standard-version": "^7.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {},
  "size-limit": [
    {
      "limit": "400 ms",
      "path": "dist/esm/aspectjs-core.mjs"
    }
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
