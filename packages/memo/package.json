{
  "name": "@aspectjs/memo",
  "version": "0.1.1",
  "description": "Add memoize pattern with a simple annotation",
  "license": "MIT",
  "main": "dist/bundles/umd/memo.umd.js",
  "unpkg": "dist/bundles/umd/memo.umd.min.js",
  "module": "dist/bundles/fesm2015/memo.js",
  "esm2015": "dist/bundles/esm2015/memo.js",
  "fesm2015": "dist/bundles/fesm2015/memo.js",
  "sideEffects": false,
  "typings": "dist/memo.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "clean": "rimraf dist",
    "typings": "tsc -b tsconfig.types.json && api-extractor run --local --verbose",
    "test": "nyc jasmine --config=jasmine.json",
    "dev": "nodemon  -w src --ext ts --exec 'npm run lint:auto && npm run test && npm run size'",
    "size": "size-limit",
    "lint:auto": "eslint -c ./.eslintrc.js --ignore-path .gitignore '**/*.{js,ts,tsx}' --quiet --fix",
    "lint": "eslint -c ./.eslintrc.js --ignore-path .gitignore '**/*.{js,ts,tsx}' --quiet"
  },
  "peerDependencies": {
    "@aspectjs/core": "0.1.1"
  },
  "optionalDependencies": {
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@aspectjs/core": "^0.1.1",
    "@emotion/hash": "^0.8.0",
    "@size-limit/preset-big-lib": "^4.5.4",
    "fake-indexeddb": "^3.0.0",
    "localstorage-ponyfill": "^1.0.2",
    "moment": "^2.24.0",
    "nodemon": "^2.0.2"
  },
  "size-limit": [
    {
      "limit": "400 ms",
      "path": "dist/bundles/umd/memo.umd.min.js"
    }
  ],
  "keywords": [
    "memo",
    "aspect",
    "cache",
    "localstorage",
    "websql",
    "AOP"
  ],
  "author": "Nicolas Thierion <nicolas.thierion@outlook.com>",
  "homepage": "https://nicolasthierion.github.io/aspectjs/#/",
  "files": [
    "dist",
    "*/dist",
    "*/package.json"
  ],
  "directories": {
    "lib": "src",
    "test": "tests",
    "dist": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasThierion/aspectjs.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasThierion/aspectjs/issues"
  },
  "dependencies": {
    "@emotion/hash": "^0.8.0",
    "fast-copy": "^2.1.0",
    "flatted": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
