{
  "name": "memo",
  "version": "1.0.0",
  "description": "Add memoize pattern with a simple annotation",
  "license": "MIT",
  "main": "bundles/memo.umd.js",
  "unpgk": "bundles/memo.umd.min.js",
  "module": "esm/memo.mjs",
  "sideEffects": false,
  "typings": "./esm/index.d.ts",
  "scripts": {
    "build": "rollup --config ./rollup.config.js",
    "test": "nyc jasmine-ts --config=jasmine.json",
    "test:integration": "cd tests/angular && npm install && npm install ../../dist && npm run test",
    "size": "rollup --config ./rollup.config.js && size-limit",
    "lint:auto": "eslint -c ./.eslintrc.js --ignore-path .gitignore '**/*.{js,ts,tsx}' --quiet --fix",
    "lint": "eslint -c ./.eslintrc.js --ignore-path .gitignore '**/*.{js,ts,tsx}' --quiet"
  },
  "dependencies": {
    "@aspectjs/core": "1.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-big-lib": "^4.0.1",
    "size-limit": "^4.0.1"
  },
  "size-limit": [
    {
      "limit": "400 ms",
      "path": "dist/esm/core.mjs"
    }
  ],
  "keywords": [
    "memo",
    "aspect",
    "cache",
    "localstorage",
    "websql",
    "AOP"
  ],
  "author": "Nicolas Thierion <thierion.nicolas@gmail.com>",
  "homepage": "https://nicolasthierion.github.io/aspectjs/#/",
  "directories": {
    "src": "src"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasThierion/aspectjs.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasThierion/aspectjs/issues"
  }
}
