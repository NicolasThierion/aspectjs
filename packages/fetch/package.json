{
  "name": "@aspectjs/fetch",
  "version": "0.1.3",
  "description": "Annotation-based fetch client",
  "license": "MIT",
  "main": "dist/bundles/fetch.umd.js",
  "unpkg": "dist/bundles/fetch.umd.min.js",
  "module": "dist/fesm2015/fetch.js",
  "fesm2015": "dist/fesm2015/fetch.js",
  "esm2015": "dist/esm2015/fetch/public_api.js",
  "sideEffects": false,
  "typings": "dist/fetch.d.ts",
  "scripts": {
    "build": "tsc -b -v support/observables && tsc -b -v && rollup --config rollup.config.js",
    "clean": "rimraf dist *.tsbuildinfo",
    "typings": "api-extractor run --local --verbose",
    "test": "nyc jasmine --config=jasmine.json",
    "dev": "nodemon  -w src --ext ts --exec 'npm run lint:auto && npm run test && npm run size'",
    "size": "size-limit",
    "lint:auto": "eslint -c ./.eslintrc.js --ignore-path .gitignore '**/*.{js,ts,tsx}' --quiet --fix",
    "lint": "eslint -c ./.eslintrc.js --ignore-path .gitignore '**/*.{js,ts,tsx}' --quiet"
  },
  "peerDependencies": {
    "@aspectjs/core": "0.1.1"
  },
  "optionalDependencies": {
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@aspectjs/core": "^0.1.3",
    "@emotion/hash": "^0.8.0",
    "@size-limit/preset-big-lib": "^4.5.4",
    "fake-indexeddb": "^3.0.0",
    "localstorage-ponyfill": "^1.0.2",
    "moment": "^2.24.0",
    "nodemon": "^2.0.2"
  },
  "size-limit": [
    {
      "limit": "400 ms",
      "path": "dist/bundles/fetch.umd.min.js"
    }
  ],
  "keywords": [
    "fetch",
    "aspect",
    "ajax",
    "annotation",
    "decorator",
    "AOP"
  ],
  "author": "Nicolas Thierion <nicolas.thierion@outlook.com>",
  "homepage": "https://nicolasthierion.github.io/aspectjs/#/",
  "files": [
    ".README",
    "dist/bundles",
    "dist/esm2015",
    "dist/fesm2015",
    "**/package.json",
    "./memo.d.ts",
    "./memo-observable-support.d.ts"
  ],
  "directories": {
    "lib": "src",
    "test": "tests",
    "dist": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasThierion/aspectjs.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasThierion/aspectjs/issues"
  },
  "dependencies": {
    "@emotion/hash": "^0.8.0",
    "fast-copy": "^2.1.0",
    "flatted": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
