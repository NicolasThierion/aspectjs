{
  "name": "@aspectjs/http",
  "version": "0.3.8",
  "description": "Http annotations to write HTTP clients & servers powered by AOP",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasThierion/aspectjs.git",
    "directory": "packages/http"
  },
  "scripts": {
    "test": "npm run clean && npm run test:lint && npm run test:unit",
    "test:lint": "eslint \"src/**/*.{ts,js,json}\"",
    "test:unit": "jest --runInBand",
    "lint": "eslint \"src/**/*.{ts,js,json}\" --fix",
    "build": "rollup --config ./rollup.config.cjs",
    "clean": "rimraf ./dist && rimraf ./*.tgz"
  },
  "author": "Nicolas Thierion <nicolas.thierion@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NicolasThierion/aspectjs/issues"
  },
  "homepage": "https://aspectjs.gitlab.io/",
  "type": "module",
  "sideEffects": false,
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/http.js",
      "es2020": "./fesm2020/http.mjs",
      "node": "./cjs/http.cjs",
      "require": "./cjs/http.cjs",
      "default": "./cjs/http.cjs",
      "unpkg": "./umd/http.umd.min.js"
    }
  },
  "module": "./fesm2020/http.mjs",
  "main": "./umd/http.umd.min.js",
  "dependencies": {
    "@emotion/hash": "^0.9.1"
  },
  "devDependencies": {
    "@types/whatwg-fetch": "^0.0.33",
    "jest": "^29.5.0",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "@aspectjs/common": "^0.3.2",
    "@aspectjs/core": "^0.3.2"
  }
}
