{
  "name": "@aspectjs/docs",
  "private": true,
  "version": "0.4.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "npm run docs:dev",
    "clean": "rimraf ./src/.vuepress/.cache ./src/.vuepress/.temp",
    "docs:build": "npm run typedoc && vuepress build src --dest dist",
    "docs:clean-dev": "vuepress dev src --clean-cache",
    "typedoc": "tsx ./src/.vuepress/run-typedoc.ts",
    "docs:dev": "vuepress dev src",
    "docs:update-package": "npx vp-update"
  },
  "devDependencies": {
    "@vuepress/client": "^2.0.0-beta.67",
    "jsdom": "^22.1.0",
    "katex": "^0.16.9",
    "mathjax-full": "^3.2.2",
    "reveal.js": "^4.6.1",
    "tsx": "^4.19.1",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "vue": "^3.3.4",
    "vuepress": "^2.0.0-beta.67",
    "vuepress-plugin-search-pro": "2.0.0-beta.239",
    "vuepress-theme-hope": "2.0.0-beta.239"
  },
  "dependencies": {
    "find-up": "^6.3.0",
    "glob": "^10.3.10",
    "json5": "^2.2.3"
  }
}
