{
  "name": "@aspectjs/docs",
  "private": true,
  "version": "0.3.12",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "npm run docs:dev",
    "clean": "rimraf ./src/.vuepress/.cache ./src/.vuepress/.temp",
    "docs:build": "npm run typedoc && vuepress build src --dest dist",
    "docs:clean-dev": "vuepress dev src --clean-cache",
    "typedoc": "node --loader ts-node/esm ./src/typedoc.ts src",
    "docs:dev": "vuepress dev src",
    "docs:update-package": "npx vp-update"
  },
  "devDependencies": {
    "@vuepress/client": "^2.0.0-beta.67",
    "jsdom": "^22.1.0",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "vue": "^3.3.4",
    "vuepress": "^2.0.0-beta.67",
    "vuepress-plugin-search-pro": "^2.0.0-beta.237",
    "vuepress-plugin-typedoc": "^0.13.0",
    "vuepress-theme-hope": "^2.0.0-beta.237"
  },
  "dependencies": {
    "find-up": "^6.3.0",
    "glob": "^10.3.10",
    "json5": "^2.2.3"
  }
}
