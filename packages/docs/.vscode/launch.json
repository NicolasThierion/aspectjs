  "version": "0.2.0",
  "configurations": [
    {
      "name": "npm start",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["start"],
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "Launch typedoc",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "typedoc"],
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
