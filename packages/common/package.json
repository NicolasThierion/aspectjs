{
  "name": "@aspectjs/common",
  "version": "0.3.0",
  "description": "Aspectjs - commonly needed factories & utils",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint \"src/**/*.{ts,js,json}\"",
    "test:unit": "jest --runInBand",
    "lint": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "doc": "typedoc src/index.ts",
    "build:common": "rollup --config ./rollup.config.js",
    "build:utils": "rollup --config ./utils/rollup.config.js",
    "build:testing": "rollup --config ./testing/rollup.config.js",
    "build": "rimraf dist && npm run build:utils && npm run build:common && npm run build:testing"
  },
  "author": "Nicolas Thierion <nicolas.thierion@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NicolasThierion/aspectjs/issues"
  },
  "homepage": "https://github.com/NicolasThierion/aspectjs#readme",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/common.d.ts",
      "esm2020": "./dist/esm2020/common.mjs",
      "es2020": "./dist/fesm2020/common.mjs",
      "node": "./dist/fesm2020/common.mjs",
      "require": "./dist/common.cjs",
      "default": "./dist/common.cjs",
      "unpkg": "./dist/bundles/common.umd.min.js"
    }
  },
  "main": "./dist/bundles/common.umd.js"
}
